# Supabase Configuration 
# Get these from your Supabase project settings  
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url  
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_OR_ANON_KEY=your_supabase_anon_key  
SUPABASE_SERVICE_KEY=your_supabase_service_role_key 
  
# AI Services  
# Google Gemini API key for AI document generation and Sangam AI  
GEMINI_API_KEY=your_google_gemini_api_key  
  
# OpenAI API key for vector embeddings (Sangam AI)  
OPENAI_API_KEY=your_openai_api_key 
  
# Email Configuration (Optional)  
# SMTP settings for email notifications and password reset  
SMTP_HOST=your_smtp_host  
SMTP_PORT=587  
SMTP_USER=your_smtp_username  
SMTP_PASS=your_smtp_password  
FROM_EMAIL=your_from_email 
  
# Application Configuration  
# Default tenant ID for the application  
NEXT_PUBLIC_TENANT=your_default_tenant_id  
  
# Development Configuration (Optional)  
# Set to 'development' for local development  
NODE_ENV=development 
  
# Sangam AI Configuration (Optional)  
# Similarity threshold for vector search (0.0 to 1.0)  
SANGAM_SIMILARITY_THRESHOLD=0.3  
  
# Maximum number of results for vector search  
SANGAM_MAX_RESULTS=15 
